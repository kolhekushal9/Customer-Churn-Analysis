    "import pandas as pd\n",
    "\n",
    "df = pd.read_csv(\"telco_churn_cleaned.csv\")\n",
    "\n",
    "# Feature engineering\n",
    "df['AvgMonthlySpend'] = df['TotalCharges'] / (df['tenure'] + 1)\n",
    "\n",
    "df.to_csv(\"telco_churn_featured.csv\", index=False)\n",
    "df.head()\n"
